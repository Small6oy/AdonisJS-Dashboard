<!DOCTYPE html>
<html lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>AdonisJs - Dashboard</title>

    <!-- Bootstrap -->
    <link href="bower/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="bower/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="bower/nprogress/nprogress.css" rel="stylesheet">
    <!-- Animate.css -->
    <link href="bower/animate.css/animate.min.css" rel="stylesheet">

    <!-- Custom Theme Style -->
    <link href="css/custom.min.css" rel="stylesheet">
  </head>

  <body class="login">
  
    <div class="login_wrapper">
        <section class="animated fadeIn login_content">
          {% block content %}{% endblock %}
        </section>
    </div>
    
    <!-- jQuery -->
    <script src="bower/jquery/dist/jquery.min.js"></script>
    <!-- NProgress -->
    <script src="bower/nprogress/nprogress.js"></script>

    <!-- Custom Theme Scripts -->
    <script>
      // NProgress
      if (typeof NProgress != 'undefined') {
          $(document).ready(function() {
              NProgress.start();
          });

          $(window).load(function() {
              NProgress.done();
          });
      }
    </script>
  </body>
</html>
